{
  "name": "stream-boosters",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "clean": "rimraf dist",
    "build:local": "rimraf dist && astro build && cp src/php/* dist/",
    "copy:xampp": "cp -r dist/* /opt/lampp/htdocs/stream-boosters/",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "lint": "eslint .",
    "format": "prettier --write ./src/**/*.{js,jsx,ts,tsx,json,md}"
  },
  "dependencies": {
    "@astrojs/react": "^4.3.0",
    "@fontsource/sora": "^5.2.6",
    "@hookform/resolvers": "^5.1.1",
    "@iconify-json/material-symbols": "^1.2.29",
    "@tailwindcss/vite": "^4.1.11",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "astro": "^5.11.0",
    "astro-icon": "^1.1.5",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.60.0",
    "tailwindcss": "^4.1.11",
    "zod": "^3.25.74"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@typescript-eslint/parser": "^8.36.0",
    "astro-eslint-parser": "^1.2.2",
    "eslint": "^9.31.0",
    "eslint-plugin-astro": "^1.3.1",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "prettier": "^3.6.2",
    "prettier-plugin-astro": "^0.14.1",
    "rimraf": "^6.0.1",
    "vitest": "^3.2.4"
  }
}